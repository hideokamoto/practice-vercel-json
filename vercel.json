{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    },
    {
      "source": "/demo",
      "destination": "https://github.com/vercel/vercel"
    },
    {
      "source": "/demo1/(.*)",
      "destination": "https://github.com/vercel/vercel/"
    },
    {
      "source": "/demo2/:path*",
      "destination": "https://github.com/vercel/vercel/:path*"
    }
  ]
}